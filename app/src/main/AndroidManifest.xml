<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trabajo1">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.trabajofct.Activities.AnadirUsuariosActivity"></activity>
        <activity android:name="com.example.trabajofct.Activities.AnadirAsignatura" />
        <activity android:name="com.example.trabajofct.Activities.AnadirGrupo" />
        <activity android:name="com.example.trabajofct.Activities.GestionarActivity" />
        <activity android:name="com.example.trabajofct.Activities.RecuperarContrasena" />
        <activity android:name="com.example.trabajofct.Activities.RegisterActivity" />
        <activity android:name="com.example.trabajofct.Utils.Util" />
        <activity android:name="com.example.trabajofct.Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.trabajofct.Activities.MainActivity" />
    </application>

</manifest>